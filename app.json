{
  "expo": {
    "name": "InsuLink",
    "slug": "InsuLink",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.calummullen.InsuLink"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.calummullen.InsuLink"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/hussar/HussarPrintA-M9nY.otf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "187ea514-5ee2-4243-a4e3-bebfcf92ff23"
      }
    }
  },
  "android": {
    "package": "com.yourcompany.yourappname",
    "versionCode": 1
  }
}
